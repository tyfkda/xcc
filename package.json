{
  "name": "Wcc",
  "version": "0.1.0",
  "description": "C-compiler for wasm",
  "main": "index.js",
  "scripts": {
    "start": "npm run default",
    "default": "npm run build-assets && gulp",
    "build": "gulp build",
    "build-assets": "make assets",
    "watch": "gulp watch",
    "server": "webpack-dev-server --content-base public",
    "test": "gulp test",
    "lint": "gulp lint",
    "lint:fix": "eslint --fix src/www/**/*.ts",
    "release": "gulp release",
    "clean": "gulp clean"
  },
  "author": "tyfkda",
  "repository": {
    "type": "git",
    "url": "https://github.com/tyfkda/xcc"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@babel/core": "~7.20.7",
    "@babel/preset-env": "~7.20.2",
    "@babel/register": "~7.18.9",
    "@types/ace": "~0.0.48",
    "@types/jest": "~29.2.4",
    "@types/node": "~16.11.11",
    "@typescript-eslint/eslint-plugin": "~5.47.0",
    "@typescript-eslint/parser": "~5.47.0",
    "browser-sync": "~2.27.12",
    "clone": "~2.1.2",
    "commander": "~9.4.1",
    "eslint": "~8.30.0",
    "gulp": "~4.0.2",
    "gulp-cssnano": "~2.1.3",
    "gulp-ejs": "~5.1.0",
    "gulp-eslint": "~6.0.0",
    "gulp-htmlmin": "~5.0.1",
    "gulp-jest": "~4.0.4",
    "gulp-sass": "~5.1.0",
    "gulp-util": "~3.0.8",
    "jest": "~29.3.1",
    "jest-cli": "~29.3.1",
    "sass": "~1.57.1",
    "split.js": "~1.6.5",
    "tmp": "~0.2.1",
    "ts-jest": "~29.0.3",
    "ts-loader": "~9.4.2",
    "ts-node": "~10.9.1",
    "typescript": "~4.9.4",
    "webpack": "~5.75.0",
    "webpack-stream": "~7.0.0"
  },
  "dependencies": {
    "core-js": "~3.26.1"
  }
}
